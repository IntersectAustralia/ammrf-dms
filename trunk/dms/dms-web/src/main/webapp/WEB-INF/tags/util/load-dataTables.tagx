<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" version="2.0">

    <jsp:output omit-xml-declaration="yes" />

    <spring:url value="/resources/js/jquery/jquery.dataTables.js" var="jquery_dataTables_plugin_url"/>
    <spring:url value="/resources/styles/dataTables_jui.css" var="dataTables_theme_url"/>
    
    <script src="${jquery_dataTables_plugin_url}" type="text/javascript" ><!-- //required for FF3 and Opera --></script>
    <link rel="stylesheet" type="text/css" href="${dataTables_theme_url}"><!-- //required for FF3 and Opera --></link>
</jsp:root>